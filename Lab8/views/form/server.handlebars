<div class="row">
  <div class="col-sm-12">
    <form class="server-form" method="post">
      <div class="form-group">
        <label for="string1">First String</label>
        <input type="string" class="form-control" id="string1" name="string1" placeholder="First String" value="{{firstString}}">
      </div>
      <div class="form-group">
        <label for="string2">Second String</label>
        <input type="string" class="form-control" id="string2" name="string2" placeholder="Second String" value="{{secondString}}">
      </div>
      <div class="form-group">
        <label for="number1">First Number</label>
        <input type="number" class="form-control" id="number1" name="number1" placeholder="First Number" value="{{firstNumber}}">
      </div>
      <div class="form-group">
        <label for="number2">Second Number</label>
        <input type="number" class="form-control" id="number2" name="number2" placeholder="Second Number" value="{{secondNumber}}">
      </div>
      {{#if error }}
      <div class="form-group">
        <div class="alert alert-danger">
          {{ error }}
        </div>
      </div>
      {{/if}}
      <div class="form-group">
        <button type="submit" class="btn btn-default">Submit</button>
      </div>
      {{#if result }}
      <div class="form-group">
        <div class="alert alert-success">
          {{ result }}
        </div>
      </div>
      {{/if}}
    </form>
  </div>
</div>